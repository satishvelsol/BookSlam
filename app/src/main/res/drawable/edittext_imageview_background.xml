<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:height="50dp"
        android:width="50dp">
        <shape android:shape="rectangle">
            <size
                android:width="50dp"
                android:height="50dp"/>
            <stroke
                android:width="1dp"
                android:color="#aeaeae"/>
            <solid
                android:color="@color/blue"/>
        </shape>
    </item>
    <item
        android:left="45dp"
        android:top="10dp"
        android:bottom="10dp">
        <bitmap android:src="@drawable/right"/>
    </item>
</layer-list>